{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "01 คลังโครงการ"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "project_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "inversedBy": "projects",
      "required": true
    },
    "project_sub_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-sub-category.project-sub-category",
      "inversedBy": "projects",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "main_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "ProjectDetail": {
      "type": "dynamiczone",
      "components": [
        "project-detail.project-sub-images",
        "project-detail.project-description"
      ],
      "required": true
    },
    "urls": {
      "type": "dynamiczone",
      "components": ["url-item.urls"]
    }
  }
}
