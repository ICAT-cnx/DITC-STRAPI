{
  "kind": "collectionType",
  "collectionName": "infos",
  "info": {
    "singularName": "info",
    "pluralName": "infos",
    "displayName": "Info"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "main_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "NewsDetail": {
      "type": "dynamiczone",
      "components": [
        "news-detail.news-sub-images",
        "news-detail.news-description"
      ],
      "required": true
    }
  }
}
